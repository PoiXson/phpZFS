{
	"name":        "pxn/phpzfs",
	"description": "ZFS API Library",
	"version":     "0.1.0",
	"type":        "library",
	"homepage":    "http://poixson.com/",
	"license":     "GPL-3",
	"support": {
		"issues": "https://github.com/PoiXson/phpZFS/issues",
		"source": "https://github.com/PoiXson/phpZFS",
		"irc":    "irc://irc.esper.net/poixson"
	},
	"authors": [{
		"name":  "lorenzop",
		"role":  "Developer"
	}],
	"autoload": {
		"psr-4": {
			"pxn\\phpZFS\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"pxn\\phpZFS\\tests\\":    "tests/",
			"pxn\\phpZFS\\examples\\": "examples/"
		}
	},
	"archive": {
		"exclude": [
			"tests/",
			"example/"
		]
	},
	"require": {
		"php":          "5.6.*",
		"pxn/phputils": "^0.1.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.7.5"
	}
}
